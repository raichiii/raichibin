Host *
    IdentityFile ~/.ssh/id_ed25519
    AddKeysToAgent yes
    TCPKeepAlive yes
    StrictHostKeyChecking no
    # Control Master Connection, for faster connection
    # https://qiita.com/suin/items/1708dc78fc412297f885#comments
    ControlMaster auto
    ControlPath ~/.ssh/mux-%r@%h:%p
    ControlPersist 4h
